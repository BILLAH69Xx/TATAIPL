	<script>
  const playerElement = document.getElementById('player');
  let player;

  function playVideo(event) {
    event.preventDefault();
    const videoSource = event.currentTarget.getAttribute('data-video-source');
    const posterImage = 'https://raw.githubusercontent.com/BILLAH69Xx/TATAIPL/main/dda8bac8-fd53-47c0-be1c-c6334e2ece79.jpg';

    if (player) {
      player.configure({
        source: videoSource,
        poster: posterImage,
      });
    } else {
      player = new Clappr.Player({
        source: videoSource,
        mimeType: 'application/x-mpegURL',
        autoPlay: true,
        disableVideoTagContextMenu: true,
        height: '100%',
        width: '100%',
        poster: posterImage,
      });
      player.attachTo(playerElement);
    }
  }
</script>


